{
  "buildCommand": "yarn install && yarn build",
  "outputDirectory": "build",
  "installCommand": "yarn install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "16.20.2",
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  }
}
